//17.Write a java program to use interface

interface Animal
{
	void sound();
	void eat();
}

class Dog implements Animal
{
	public void sound()
	{
		System.out.println("Barkig..");
	}
	public void eat()
	{
		System.out.println("Eating..");
	}
}

class Prg17_Interface
{
	public static void main(String[] args)
	{
		Dog d = new Dog();
		d.sound();
		d.eat();
	}
}
